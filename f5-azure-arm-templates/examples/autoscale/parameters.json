    "provisionApp": {
      "value": "No"
    },
    "serviceDiscoveryApiKey": {
      "value": ""
    },
    "tagValues": {
      "value": {
        "application": "APP",
        "cost": "COST",
        "environment": "ENV",
        "group": "GROUP",
        "owner": "OWNER"
      }
