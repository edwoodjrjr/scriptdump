{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "templateBaseUrl": {
      "value": "https://raw.githubusercontent.com/edwoodjrjr/scriptdump/master/"
    },
    "artifactLocation": {
      "value": "f5-azure-arm-templates/examples/"
    },
    "customDoDeclarationUrl": {
      "value": "OPTIONAL"
    },
    "customAs3DeclarationUrl": {
      "value": "OPTIONAL"
